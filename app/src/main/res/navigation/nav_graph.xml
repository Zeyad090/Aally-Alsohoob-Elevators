<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="zeyad.app.aallyalsohoobelevators.welcome.WelcomeFragment"
        tools:layout="@layout/fragment_welcome">


        <action
            android:id="@+id/action_welcomeFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/startFragment"
        android:name="zeyad.app.aallyalsohoobelevators.start.StartFragment"
        tools:layout="@layout/fragment_start">
        <action
            android:id="@+id/action_startFragment_to_typesOfCabinOfElevatorsFragment"
            app:destination="@id/typesOfCabinOfElevatorsFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_startFragment_to_typesOfTheElevatorsFragment"
            app:destination="@id/typesOfTheElevatorsFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_startFragment_to_requestFragment"
            app:destination="@id/requestFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_startFragment_to_servicesFragment"
            app:destination="@id/servicesFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_startFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_startFragment_to_seeMyRequestsFragment"
            app:destination="@id/seeMyRequestsFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/servicesFragment"
        android:name="zeyad.app.aallyalsohoobelevators.services.ServicesFragment"
        tools:layout="@layout/fragment_services" >
        <action
            android:id="@+id/action_servicesFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/requestFragment"
        android:name="zeyad.app.aallyalsohoobelevators.request.RequestFragment"
        tools:layout="@layout/fragment_request" >
        <action
            android:id="@+id/action_requestFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
        <argument
            android:name="image"
            app:argType="string" />
        <action
            android:id="@+id/action_requestFragment_to_myRequestsFragment"
            app:destination="@id/myRequestsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_requestFragment_to_typesOfCabinOfElevatorsFragment"
            app:destination="@id/typesOfCabinOfElevatorsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/typesOfCabinOfElevatorsFragment"
        android:name="zeyad.app.aallyalsohoobelevators.cabin.TypeOfCabinOfElevatorsFragment" >
        <action
            android:id="@+id/action_typesOfCabinOfElevatorsFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_typesOfCabinOfElevatorsFragment_to_requestFragment"
            app:destination="@id/requestFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/typesOfTheElevatorsFragment"
        android:name="zeyad.app.aallyalsohoobelevators.types.TypesOfTheElevatorsFragment"
        android:label="fragment_types_of_the_elevators"
        tools:layout="@layout/fragment_types_of_the_elevators" >
        <action
            android:id="@+id/action_typesOfTheElevatorsFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_typesOfTheElevatorsFragment_to_myRequestsFragment"
            app:destination="@id/myRequestsFragment" />
    </fragment>
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="zeyad.app.aallyalsohoobelevators.users.UserProfileFragment"
        tools:layout="@layout/fragment_user_profile">
        <action
            android:id="@+id/action_userProfileFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>


    <fragment
        android:id="@+id/signInFragment"
        android:name="zeyad.app.aallyalsohoobelevators.users.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_signInFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_signInFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="zeyad.app.aallyalsohoobelevators.users.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="zeyad.app.aallyalsohoobelevators.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/from_right" />
    </fragment>
    <fragment
        android:id="@+id/myRequestsFragment"
        android:name="zeyad.app.aallyalsohoobelevators.MyRequestsFragment"
        android:label="fragment_my_requests"
        tools:layout="@layout/fragment_my_requests" >
        <action
            android:id="@+id/action_myRequestsFragment_to_requestFragment"
            app:destination="@id/requestFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <argument
            android:name="image"
            app:argType="string" />
        <argument
            android:name="types"
            app:argType="string" />
        <argument
            android:name="floor"
            app:argType="string" />
        <argument
            android:name="machine"
            app:argType="string" />
        <argument
            android:name="country"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/seeMyRequestsFragment"
        android:name="zeyad.app.aallyalsohoobelevators.SeeMyRequestsFragment"
        android:label="fragment_see_my_requests"
        tools:layout="@layout/fragment_see_my_requests" >
        <action
            android:id="@+id/action_seeMyRequestsFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
</navigation>